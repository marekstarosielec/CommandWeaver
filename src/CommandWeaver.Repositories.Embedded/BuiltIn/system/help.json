{
  "commands": [
    {
      "name": "help",
      "parameters": [
        {
          "key": "command",
          "description": "Name of command"
        }
      ],
      "operations": [
        {
          "operation": "output",
          "value": "Command-Weaver by [[{{styles[focused].value}}]]Marek Starosielec\r\n",
          "styling": "MarkupLine"
        },
        {
          "operation": "forEach",
          "list": "{{ commands }}",
          "element": "currentCommand",
          "conditions": {
            "isNull": "{{ command }}"
          },
          "operations": [
            {
              "operation": "output",
              "value": "  [[{{styles[focused].value}}]]{{ currentCommand.name }}[[/]]",
              "styling": "Markup"
            },
            {
              "operation": "output",
              "value": " - {{ currentCommand.description }}",
              "styling": "Markup",
              "conditions": {
                "isNotNull": "{{ currentCommand.description }}"
              }
            },
            {
              "operation": "output",
              "value": " ",
              "styling": "MarkupLine"
            }
          ]
        },
        {
          "operation": "output",
          "value": "\r\nTo display additional help about specific command, use command [[{{styles[focused].value}}]]help --command [[{{styles[focused].value}},italic]]<command-name>",
          "styling": "Markup",
          "conditions": {
            "isNull": "{{ command }}"
          }
        },
        {
          "operation": "output",
          "value": "Usage: [[{{styles[focused].value}}]] {{ commands[{{command}}].name }}",
          "styling": "Markup",
          "conditions": {
            "isNotNull": "{{ command }}"
          }
        },
        {
          "operation": "forEach",
          "list": "{{ commands[{{command}}].parameters }}",
          "element": "commandParameter",
          "conditions": {
            "isNotNull": "{{ commands[{{command}}].parameters }}"
          },
          "operations": [
            {
              "operation": "output",
              "value": " --{{commandParameter.key}}",
              "styling": "Markup"
            }
          ]
        }
      ]
    }
  ]
}