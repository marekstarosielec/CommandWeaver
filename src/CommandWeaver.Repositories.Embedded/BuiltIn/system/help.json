{
  "commands": [
    {
      "name": "help",
      "operations": [
        {
          "operation": "output",
          "value": "Command-Weaver by [[#FFFFFF]]Marek Starosielec\r\n",
          "styling": "MarkupLine"
        },
        {
          "operation": "forEach",
          "list": "{{ commands }}",
          "element": "command",
          "operations": [
            {
              "operation": "output",
              "value": "  [[#FFFFFF]]{{ command.key }}[[/]]",
              "styling": "Markup"
            },
            {
              "operation": "output",
              "value": " - {{ command.description }}",
              "styling": "Markup",
              "conditions": {
                "isNotNull": "{{ command.description }}"
              }
            },
            {
              "operation": "output",
              "value": " ",
              "styling": "MarkupLine"
            }
          ]
        }
      ]
    }
  ]
}