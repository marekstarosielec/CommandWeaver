{
  "commands": [
    {
      "name": "cmd",
      "parameters": [
        {
          "key": "name",
          "otherNames": ["n"],
          "description": "Name of command",
          "required": true
        }
      ],
      "operations": [
        {
          "operation": "terminate",
          "message": "Command {{name}} does not exist",
          "conditions": {
            "isNull": "{{ commands[{{name}}] }}"
          }
        },
        {
          "operation": "output",
          "value": "Command [[#0000FF]]{{ name }}[[/]] in [[u]]{{commands[{{name}}].id}}[[/]]"
        },
        {
          "operation": "output",
          "value": "[[raw,json]]{{ commands[{{name}}].json }}[[/]]",
          "formatting": "raw"
        }
      ]
    }
  ]
}