{
  "commands": [
    {
      "name": "cmd",
      "description": "Display information about single command",
      "parameters": [
        {
          "key": "name",
          "otherNames": ["n"],
          "description": "Name of command",
          "required": true
        }
      ],
      "operations": [
        {
          "operation": "terminate",
          "message": "Command {{name}} does not exist",
          "conditions": {
            "isNull": "{{ commands[{{name}}] }}"
          }
        },
        {
          "operation": "output",
          "value": "{{ commands[{{name}}].json }}",
          "styling": "json"
        }
      ]
    }
  ]
}