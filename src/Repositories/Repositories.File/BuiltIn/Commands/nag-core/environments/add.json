{
  "name": "nag-core-environment-add",
  "parameters": [
    {
      "key": "key",
      "description": "Name of nag-core environment. Must be unique." 
    },
    {
      "key": "host",
      "description": "Nag-core host."
    }
  ],
  "operations": [
    {
      "operation": "setVariable",
      "key": "nag-core-environments[{{ key }}].url",
      "value": "{{ host }}"
    }
  ]
}